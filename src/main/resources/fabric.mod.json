{
  "schemaVersion": 1,
  "id": "mcphilben-mc",
  "version": "${version}",
  "name": "mcPhilben",
  "description": "A pulbic recreation of the Copenheimer System",
  "authors": [
    "GradientGamer",
    "Lemontech Development"
  ],
  "contact": {
    "repo": "https://github.com/GradientGamer-XD/McPhilben"
  },
  "icon": "assets/mcphilben/mcphilbenscaled.png",
